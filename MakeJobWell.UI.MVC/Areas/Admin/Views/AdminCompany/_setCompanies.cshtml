@model List<CompanyVM>
@foreach (CompanyVM item in Model)
{
    <tr>
        <td>@item.CompanyID</td>
        <td>@item.CompanyName</td>
        <td><a class="btn btn-warning" href="/Admin/AdminCompany/UpdateCompany/@item.CompanyID">Update</a></td>
        <td><a class="btn btn-danger" onclick="DeleteComp(@item.CompanyID)">Delete</a></td>
    </tr>
}
